{
  "restartable": "rs",
  "ignore": [".git", "node_modules/*"],
  "verbose": true,
  "execMap": {
    "js": "node --harmony setup/index.js"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": ["setup"],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}
